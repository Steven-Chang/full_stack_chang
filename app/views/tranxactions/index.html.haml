                    %input#productdiscountRadio2.form-check-input{type: "checkbox", value: "10% or more"}/
                    %label.form-check-label{for: "productdiscountRadio2"}
                      10% or more
                  .form-check
                    %input#productdiscountRadio1.form-check-input{type: "checkbox", value: "Less than 10%"}/
                    %label.form-check-label{for: "productdiscountRadio1"}
                      Less than 10%
          / end accordion-item
          .accordion-item
            %h2#flush-headingRating.accordion-header
              %button.accordion-button.bg-transparent.shadow-none.collapsed{"aria-controls" => "flush-collapseRating", "aria-expanded" => "false", "data-bs-target" => "#flush-collapseRating", "data-bs-toggle" => "collapse", type: "button"}
                %span.text-muted.text-uppercase.fs-12.fw-semibold Rating
                %span.badge.bg-success.rounded-pill.align-middle.ms-1.filter-badge 1
            #flush-collapseRating.accordion-collapse.collapse{"aria-labelledby" => "flush-headingRating"}
              .accordion-body.text-body
                .d-flex.flex-column.gap-2.filter-check
                  .form-check
                    %input#productratingRadio4.form-check-input{checked: "checked", type: "checkbox", value: "4 & Above Star"}/
                    %label.form-check-label{for: "productratingRadio4"}
                      %span.text-muted
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star
                      4 &amp; Above
                  .form-check
                    %input#productratingRadio3.form-check-input{type: "checkbox", value: "3 & Above Star"}/
                    %label.form-check-label{for: "productratingRadio3"}
                      %span.text-muted
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star
                        %i.mdi.mdi-star
                      3 &amp; Above
                  .form-check
                    %input#productratingRadio2.form-check-input{type: "checkbox", value: "2 & Above Star"}/
                    %label.form-check-label{for: "productratingRadio2"}
                      %span.text-muted
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star.text-warning
                        %i.mdi.mdi-star
                        %i.mdi.mdi-star
                        %i.mdi.mdi-star
                      2 &amp; Above
