                %li
                  %a.d-flex.py-1.align-items-center{href: "#"}
                    .flex-grow-1
                      %h5.fs-12.mb-0.listname Kids
          .card-body.border-bottom
            %p.text-muted.text-uppercase.fs-12.fw-medium.mb-4 Price
            #product-price-range.noUi-target.noUi-ltr.noUi-horizontal.noUi-txt-dir-ltr
              .noUi-base
                .noUi-connects
                  .noUi-connect.noUi-draggable{style: "transform: translate(0%, 0px) scale(1, 1);"}
                .noUi-origin{style: "transform: translate(-100%, 0px); z-index: 5;"}
                  .noUi-handle.noUi-handle-lower{"aria-orientation" => "horizontal", "aria-valuemax" => "1980.0", "aria-valuemin" => "0.0", "aria-valuenow" => "0.0", "aria-valuetext" => "$ 0", "data-handle" => "0", role: "slider", tabindex: "0"}
                    .noUi-touch-area
                .noUi-origin{style: "transform: translate(0%, 0px); z-index: 4;"}
                  .noUi-handle.noUi-handle-upper{"aria-orientation" => "horizontal", "aria-valuemax" => "2000.0", "aria-valuemin" => "20.0", "aria-valuenow" => "2000.0", "aria-valuetext" => "$ 2000", "data-handle" => "1", role: "slider", tabindex: "0"}
                    .noUi-touch-area
            .formCost.d-flex.gap-2.align-items-center.mt-3
              %input#minCost.form-control.form-control-sm{type: "text", value: "0"}/
              %span.fw-semibold.text-muted to
              %input#maxCost.form-control.form-control-sm{type: "text", value: "1000"}/
          .accordion-item
            %h2#flush-headingBrands.accordion-header
              %button.accordion-button.bg-transparent.shadow-none{"aria-controls" => "flush-collapseBrands", "aria-expanded" => "true", "data-bs-target" => "#flush-collapseBrands", "data-bs-toggle" => "collapse", type: "button"}
                %span.text-muted.text-uppercase.fs-12.fw-semibold Brands
                %span.badge.bg-success.rounded-pill.align-middle.ms-1.filter-badge 2
            #flush-collapseBrands.accordion-collapse.collapse.show{"aria-labelledby" => "flush-headingBrands"}
              .accordion-body.text-body.pt-0
                .search-box.search-box-sm
                  %input#searchBrandsList.form-control.bg-light.border-0{placeholder: "Search Brands...", type: "text"}/
                  %i.ri-search-line.search-icon
                .d-flex.flex-column.gap-2.mt-3.filter-check
                  .form-check
                    %input#productBrandRadio5.form-check-input{checked: "checked", type: "checkbox", value: "Boat"}/
                    %label.form-check-label{for: "productBrandRadio5"} Boat
                  .form-check
                    %input#productBrandRadio4.form-check-input{type: "checkbox", value: "OnePlus"}/
                    %label.form-check-label{for: "productBrandRadio4"} OnePlus
                  .form-check
                    %input#productBrandRadio3.form-check-input{type: "checkbox", value: "Realme"}/
                    %label.form-check-label{for: "productBrandRadio3"} Realme
                  .form-check
                    %input#productBrandRadio2.form-check-input{type: "checkbox", value: "Sony"}/
                    %label.form-check-label{for: "productBrandRadio2"} Sony
                  .form-check
                    %input#productBrandRadio1.form-check-input{checked: "checked", type: "checkbox", value: "JBL"}/
                    %label.form-check-label{for: "productBrandRadio1"} JBL
                  %div
                    %button.btn.btn-link.text-decoration-none.text-uppercase.fw-medium.p-0{type: "button"}
                      1,235
                      More
          .accordion-item
            %h2#flush-headingDiscount.accordion-header
              %button.accordion-button.bg-transparent.shadow-none.collapsed{"aria-controls" => "flush-collapseDiscount", "aria-expanded" => "true", "data-bs-target" => "#flush-collapseDiscount", "data-bs-toggle" => "collapse", type: "button"}
                %span.text-muted.text-uppercase.fs-12.fw-semibold Discount
                %span.badge.bg-success.rounded-pill.align-middle.ms-1.filter-badge 1
            #flush-collapseDiscount.accordion-collapse.collapse{"aria-labelledby" => "flush-headingDiscount"}
              .accordion-body.text-body.pt-1
                .d-flex.flex-column.gap-2.filter-check
                  .form-check
                    %input#productdiscountRadio6.form-check-input{type: "checkbox", value: "50% or more"}/
                    %label.form-check-label{for: "productdiscountRadio6"} 50% or more
                  .form-check
                    %input#productdiscountRadio5.form-check-input{type: "checkbox", value: "40% or more"}/
                    %label.form-check-label{for: "productdiscountRadio5"} 40% or more
                  .form-check
                    %input#productdiscountRadio4.form-check-input{type: "checkbox", value: "30% or more"}/
                    %label.form-check-label{for: "productdiscountRadio4"}
                      30% or more
                  .form-check
                    %input#productdiscountRadio3.form-check-input{checked: "checked", type: "checkbox", value: "20% or more"}/
