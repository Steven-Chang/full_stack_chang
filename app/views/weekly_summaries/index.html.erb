<p id="notice"><%= notice %></p>

<h1>Weekly Summaries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weekly_summaries.each do |weekly_summary| %>
      <tr>
        <td><%= link_to 'Show', weekly_summary %></td>
        <td><%= link_to 'Edit', edit_weekly_summary_path(weekly_summary) %></td>
        <td><%= link_to 'Destroy', weekly_summary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weekly Summary', new_weekly_summary_path %>
